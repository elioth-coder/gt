{
    "name": "kaoken/markdown-it-php",
    "description": "PHP version makdown-it",
    "keywords": [
        "markdown-it","markdown"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4.0",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "kaoken",
            "homepage": "https://github.com/kaoken/markdown-it-php"
        }
    ],
    "require-dev": {
        "symfony/yaml":"5.0.*",
        "ext-pthreads": "*"
    },
    "replace": {
    },
    "autoload": {
        "psr-4": {
            "Kaoken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaoken\\Test\\": "tests/"
        }
    }
}
