      integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI="
      crossorigin="anonymous"
    ></script>
    <script src="https://unpkg.com/flowbite@1.4.1/dist/flowbite.js"></script>
    {% endblock scripts %}
  </body>
</html>
